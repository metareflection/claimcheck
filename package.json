    "eval:view": "npx promptfoo view"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0"
  },
  "devDependencies": {
    "js-yaml": "^4.1.1",
    "promptfoo": "^0.120.24"
  }
}
