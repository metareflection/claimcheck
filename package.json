{
  "name": "claimcheck",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "bin": {
    "claimcheck": "./bin/claimcheck.js"
  },
  "scripts": {
    "eval": "npx promptfoo eval -c eval/promptfooconfig.yaml",
    "eval:cached": "CLAIMCHECK_EVAL_CACHED=1 npx promptfoo eval -c eval/promptfooconfig.yaml",
